            <collision name="laser_link1_geom">
                <geometry>
                    <box><size>0.1 0.1 0.1</size></box>
                </geometry>
            </collision>
            <visual name="laser_link1_visual">
                <material><script><name>Gazebo/Red</name></script></material>
                <geometry>
                    <box><size>0.01 0.01 0.01</size></box>
                </geometry>
            </visual>
            <sensor name="laser1" type="ray">
                <pose>0 0 0 0 -1.570796327 0</pose>
                <visualize>true</visualize>
                <update_rate>125</update_rate>
                <ray>
                    <scan>
                        <horizontal>
                            <samples>1</samples>
                            <resolution>1</resolution>
                            <min_angle>-0.000</min_angle>
                            <max_angle>0.000</max_angle>
                        </horizontal>
                    </scan>
                    <range>
                        <min>0.10</min>
                        <max>1.0</max>
                        <resolution>0.01</resolution>
                    </range>
                    <noise>
                        <type>gaussian</type>
                        <mean>0.0</mean>
                        <stddev>0.0001</stddev>
                    </noise>
                </ray>
                <plugin name="gazebo_ros_laser_controller1" filename="libgazebo_ros_laser.so">
                    <topicName>parabot/laser1</topicName>
                    <frameName>laser_link1</frameName>
                </plugin>
            </sensor>
        </link>
